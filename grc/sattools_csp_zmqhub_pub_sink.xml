<?xml version="1.0"?>
<block>
  <name>CSP zmqhub PUB sink</name>
  <key>sattools_csp_zmqhub_pub_sink</key>
  <category>[sattools]</category>
  <import>import sattools</import>
  <make>sattools.csp_zmqhub_pub_sink($pub_endpoint)</make>

  <param>
    <name>Publisher endpoint</name>
    <key>pub_endpoint</key>
    <value>tcp://address:port</value>
    <type>string</type>
  </param>

  <sink>
    <name>pdu</name>
    <type>message</type>
    <optional>1</optional>
  </sink>
</block>
